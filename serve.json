{
  "rewrites": [
    {
      "source": "/",
      "destination": "/home.html"
    },
    {
      "source": "/articles",
      "destination": "/articles.html"
    },
    {
      "source": "/blog/:post*",
      "destination": "/blogs/blog_template.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
